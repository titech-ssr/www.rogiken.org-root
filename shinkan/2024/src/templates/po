  <script type="text/javascript">
  $.ajax({
    type: "GET",
    url: "//blog.rogiken.org/feed/",
    cache: false,
    dataType: "xml",
    success: function(feed) {
      $(feed).find('item').slice(0, 10).each(function(){
        $('#blog-titles').append(
          $('<li>').append(
            $('<a />')
              .attr('href', $('link', this).text())
              .text($('title', this).text())
          )
        )
      });
    }
  });
  </script>
